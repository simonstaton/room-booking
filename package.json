{
  "name": "room-booking",
  "version": "1.0.0",
  "description": "Application to book meeting rooms",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:simonstaton/room-booking.git"
  },
  "contributors": [
    "Simon Staton <simon.staton@live.co.uk>"
  ],
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "cross-env NODE_ENV=production next build",
    "start": "cross-env NODE_ENV=production next start",
    "prepare": "husky install",
    "format": "prettier --check \"**/*.{js,jsx,ts,tsx,md,mdx,json,yaml,yml}\"",
    "format:fix": "pnpm run format --write",
    "lint": "pnpm run lint:scripts && pnpm run lint:markdown && pnpm run lint:styles && pnpm run lint:commit",
    "lint:scripts": "eslint ./ --ext .js,.jsx,.ts,.tsx",
    "lint:scripts:fix": "pnpm run lint:scripts -- --fix",
    "lint:styles": "stylelint \"**/*.{css,scss}\"",
    "lint:styles:fix": "pnpm run lint:styles -- --fix",
    "lint:markdown": "./node_modules/markdownlint-cli/markdownlint.js markdownlint **/*.md --ignore node_modules --ignore CHANGELOG.md --config ./.markdownlint.json",
    "lint:markdown:fix": "pnpm run lint:markdown  -- --fix",
    "lint:commit": "git log -1 --pretty=%B | commitlint",
    "commit": "cz",
    "pre-commit-lint": "lint-staged",
    "check-updated": "npx npm-check-updates",
    "test": "jest --coverage --silent",
    "test:watch": "npm run test -- --watch"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": "18.17.1"
  },
  "devDependencies": {
    "@babel/preset-env": "7.22.15",
    "@babel/preset-react": "7.22.15",
    "@babel/preset-typescript": "7.22.15",
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@svgr/webpack": "8.1.0",
    "@testing-library/jest-dom": "6.1.3",
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.4",
    "@types/node": "20.6.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.6.0",
    "@typescript-eslint/parser": "6.6.0",
    "babel-jest": "29.6.4",
    "babel-loader": "9.1.3",
    "browserslist": "4.21.10",
    "commitizen": "4.3.0",
    "cross-env": "7.0.3",
    "cssnano": "6.0.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.49.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unicorn": "48.0.1",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.6.4",
    "jest-css-modules-transform": "4.4.2",
    "jest-environment-jsdom": "29.6.4",
    "jest-junit": "16.0.0",
    "lint-staged": "14.0.1",
    "markdownlint": "0.31.0",
    "markdownlint-cli": "0.36.0",
    "postcss": "8.4.29",
    "postcss-custom-media-generator": "1.1.0",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-nested": "6.0.1",
    "postcss-preset-env": "9.1.3",
    "prettier": "3.0.3",
    "stylelint": "15.10.3",
    "stylelint-config-standard": "34.0.0",
    "stylelint-order": "6.0.3",
    "stylelint-prettier": "4.0.2",
    "typescript-plugin-css-modules": "5.0.1"
  },
  "dependencies": {
    "clsx": "2.0.0",
    "next": "13.4.19",
    "postcss-import": "15.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.2.2"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ]
}
